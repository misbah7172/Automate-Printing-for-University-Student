{
  "name": "autoprint-backend",
  "version": "1.0.0",
  "description": "AutoPrint Backend API Server",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all"
  },
  "keywords": [
    "autoprint",
    "printing",
    "api",
    "express",
    "sequelize"
  ],
  "author": "AutoPrint Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "sequelize": "^6.32.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1454.0",
    "stripe": "^13.5.0",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.7",
    "joi": "^17.9.2",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "socket.io": "^4.7.2",
    "bull": "^4.11.3",
    "node-cron": "^3.0.2",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.0",
    "multer-s3": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "sequelize-cli": "^6.6.1",
    "@types/jest": "^29.5.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/migrations/**",
      "!src/seeders/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}